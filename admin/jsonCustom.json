            "hidden": "true"
        },
        "showWhatsapp_cmb": {
            "type": "checkbox",
            "label": "whatsapp_cmb",
            "defaultSendTo": "getInstanceStart",
            "disabled": "true",
            "hidden": "true"
        },
        "showPushover": {
            "type": "checkbox",
