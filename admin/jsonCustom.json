{
    "i18n": true,
    "type": "panel",
    "items": {
        "showAlexa2": {
            "type": "checkbox",
            "label": "alexa2",
            "defaultSendTo": "getInstanceStart",
            "disabled": "true",
            "hidden": "true"
        },
