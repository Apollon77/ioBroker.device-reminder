            "defaultSendTo": "getInstanceStart",
            "disabled": "true",
            "hidden": "true"
        },
        "selectTypeFromNative": {
            "type": "selectSendTo",
            "label": "selectType",
            "attr": "target",
            "showProcess": true,
            "command": "getDataFromConfig",
            "help": "Select Speak or Announcemant"
        },
        "sendToBtn": {
            "type": "sendTo",
            "label": "get Data",
            "variant": "outlined",
            "showProcess": true,
            "command": "getValuesFromConfig",
            "jsonData": "{ \"data1\": \"${data.selectTypeFromNative}\"}",
            "useNative": true
        },
        "startVal": {
            "type": "number",
            "label": "Start Value",
            "help": "Enter volume, if \"0\", the current volume is used",
            "min": 0,
            "newLine": true
        },
        "endVal": {
            "type": "number",
            "label": "End Value",
            "help": "Enter volume, if \"0\", the current volume is used",
            "min": 0,
            "newLine": true
        },
        "standby": {
            "type": "number",
            "label": "Standby",
            "help": "Enter volume, if \"0\", the current volume is used",
            "min": 0,
            "newLine": true
        },
        "startCount": {
            "type": "number",
            "label": "Counter startvalues",
            "help": "Enter volume, if \"0\", the current volume is used",
            "min": 0,
            "newLine": true
        },
        "endCount": {
            "type": "number",
            "label": "Counter endvalues",
            "help": "Enter volume, if \"0\", the current volume is used",
            "min": 0,
            "newLine": true
        },
        "_headerAlexa": {
            "type": "header",
            "text": "alexa2",
            "size": 2
        },
        "alexa2": {
            "type": "selectSendTo",
            "label": "Alexa2 devices",
            "attr": "user",
            "command": "getDataCustomAlexa",
            "noTranslation": true,
            "multiple": true,
            "jsonData": "{\"name\": \"alexa2\"}",
            "sm": 12,
            "md": 12,
            "lg": 12,
            "newLine": true
        },
        "alexa2Msg": {
            "type": "text",
            "label": "Alexa2 Message",
            "attr": "msg",
            "sm": 12,
            "md": 12,
            "lg": 12,
            "newLine": true,
            "hidden": "!data.showAlexa2 || data.alexa2 == '' || data.alexa2 == null"
        },
